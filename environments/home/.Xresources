URxvt.perl-ext-common: font-size
URxvt.keysym.Mod4-Home: font-size:increase
URxvt.keysym.Mod4-End: font-size:decrease
URxvt.font-size.step: 1

URxvt*termName:rxvt
URxvt.background: #222222
URxvt.intensityStyles:false
URxvt.font: xft:DejaVu\ Sans\ Mono\ for\ Powerline:pixelsize=11
!:autohint=true:hinting=true:antialias=true
!URxvt.font: xft:Terminus:pixelsize=12:antialias=false:hinting=false:autohint=false
!URxvt.font: xft:Inconsolata:size=8
URxvt.letterSpace: 0
Xft*dpi:96
Xft*hinting: full
Xft.rgba: v-rgb
Xft.antialias: true
Xft.hinting: true
Xft.autohint: false
!Xft.hintstyle: hintslight
!Xft.lcdfilter: lcddefault
URxvt.depth: 32
URxvt.foreground: #E5E8E6
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.End: \033[4~
URxvt.keysym.Home: \033[1~
URxvt.scrollBar: false
URxvt.saveLines: 6000

XTerm*Background: #222222
XTerm*Foreground: #EEEEEE
XTerm*faceName: DejaVu\ Sans\ Mono\ for\ Powerline:size=8

!urxvt.color0: #3B444F
!urxvt.color1: #CD546C
!urxvt.color10: #BDC79E
!urxvt.color11: #E88773
!urxvt.color12: #94B1A3
!urxvt.color13: #BB6C73
!urxvt.color14: #9DA69E
!urxvt.color15: #D6C08F
!urxvt.color2: #A5AF86
!urxvt.color3: #CD7860
!urxvt.color4: #7A9388
!urxvt.color5: #75434C
!urxvt.color6: #767D75
!urxvt.color7: #999999
!urxvt.color8: #4A585A
!urxvt.color9: #EF545F


Xcursor.size: 16
